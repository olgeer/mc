<div class="form-horizontal">
	<div class="form-body">
		<div class="form-group">
			<label class="col-md-4 control-label">机房名称</label>
			<div class="col-md-8">
				<label class="control-label">{{room.roomname}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">机房地址</label>
			<div class="col-md-8">
				<label class="control-label" style="text-align: left;">{{room.address}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">联系人</label>
			<div class="col-md-8">
				<label class="control-label">{{room.contacts}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">联系人电话</label>
			<div class="col-md-8">
				<label class="control-label">{{room.contacts_tel}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">带宽</label>
			<div class="col-md-8">
				<label class="control-label">{{room.broad_band}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">运营商</label>
			<div class="col-md-8">
				<label class="control-label">{{room.line}}</label>
			</div>
		</div>
	</div>
</div>