<div class="form-horizontal">
	<div class="form-body">
		<div class="form-group">
			<label class="col-md-4 control-label">服务标签id</label>
			<div class="col-md-8">
				<label class="control-label">{{business.business_id}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">服务方标签名称</label>
			<div class="col-md-8">
				<label class="control-label">{{business.business_name}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">需求方标签名称</label>
			<div class="col-md-8">
				<label class="control-label">{{business.req_name}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">上级标签id</label>
			<div class="col-md-8">
				<label class="control-label">{{business.parent_id}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">标签等级</label>
			<div class="col-md-8">
				<label class="control-label">{{business.business_rank}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">标签类型</label>
			<div class="col-md-8">
				<label class="control-label">{{business.business_type}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">创建时间</label>
			<div class="col-md-8">
				<label class="control-label">{{business.create_time}}</label>
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-4 control-label">需求明细标准化提示</label>
			<div class="col-md-8">
				<label class="control-label">{{business.business_memo}}</label>
			</div>
		</div>
	</div>
</div>